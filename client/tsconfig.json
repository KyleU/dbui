{
  "compilerOptions": {
    "listFiles": false,
    "module": "system",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "outFile": "../web/assets/dbui.js",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "target": "ES3"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
